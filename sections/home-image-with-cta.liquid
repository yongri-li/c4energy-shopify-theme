<section id="home-image-with-cta" role="contentinfo">
	<div id="home-image-with-cta--wrapper">    
    <div id="home-image-with-cta--content">
      <div id="home-image-with-cta--content-left">
        <div id="home-image-with-cta--content-left--wrapper">
          {% if section.settings.home_image_with_cta_title != blank %}
            <h2 class="text-upper image-with-cta-title">{{ section.settings.home_image_with_cta_title }}</h2>
          {% endif %}

          {% if section.settings.home_image_with_cta_label != blank and section.settings.home_image_with_cta_url != blank %}
            <a class="c4-btn yellow home-image-with-cta--cta" href="{{ section.settings.home_image_with_cta_url }}">{{ section.settings.home_image_with_cta_label }}{% if section.settings.home_image_with_cta_sublabel != blank %}<span>{{ section.settings.home_image_with_cta_sublabel }}</span>{% endif %}{% if section.settings.home_image_with_cta_membership_text != blank %}<span class="small">{{ section.settings.home_image_with_cta_membership_text }}</span>{% endif %}</a>
          {% endif %}
        </div>
      </div>
      <div id="home-image-with-cta--content-right">
        {% if section.settings.home_image_with_cta_image != blank %}
          <img src="{{ section.settings.home_image_with_cta_image | img_url: 'master' }}" class="objfit home-image-with-cta--image">
        {% endif %}
      </div>
    </div>
	</div>
</section>



{% schema %}
{
  "name": "Home Image with CTA",
  "settings": [
    {
      "type": "text",
      "id": "home_image_with_cta_title",
      "label": "Title"
    },
    {
      "type": "image_picker",
      "id": "home_image_with_cta_image",
      "label": "Image"
    },
    {
      "type": "text",
      "id": "home_image_with_cta_label",
      "label": "CTA Label"
    },
    {
      "type": "text",
      "id": "home_image_with_cta_sublabel",
      "label": "CTA Sublabel"
    },
    {
      "type": "text",
      "id": "home_image_with_cta_membership_text",
      "label": "CTA Membership Text"
    },
    {
      "type": "url",
      "id": "home_image_with_cta_url",
      "label": "CTA Url"
    }
  ],
  "presets": [
    {
      "category": "Homepage",
      "name": "Home Image with CTA",
      "settings": {}
    }
  ]
}
{% endschema %}